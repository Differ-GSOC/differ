<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
			   http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd
			   http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
  
  <util:set id="identificationProperties">
    <value>MIME type</value>
    <value>Type of format</value>
    <value>Version of format</value>
    <value>Version</value>
  </util:set>
  
  <util:set id="validationProperties">
    <value>Validation (well formed and valid)</value>
  </util:set>
  
  <util:set id="characterizationProperties">
    <value>Pixels per second</value>
    <value>Gamma</value>
    <value>Depth of blue channel</value>
    <value>Depth of green channel</value>
    <value>Depth of red channel</value>
    <value>Copyright</value>
    <value>Compression quality</value>
    <value>File name</value>
    <value>File path</value>
    <value>Color depth</value>
    <value>Color space</value>
    <value>Color type</value>
    <value>Universal unique identifier (UUID)</value>
    <value>File size</value>
    <value>Image height</value>
    <value>Image width</value>
    <value>File created</value>
    <value>File last modified</value>
    <value>Compression</value>
    <value>Compression scheme</value>
    <value>Resolution horizontal</value>
    <value>Resolution vertical</value>
    <value>Number of channels</value>
    <value>Number of pixels</value>
    <value>Bits per sample</value>
    <value>ICC profile</value>
    <value>Samples per pixel</value>
    <value>Orientation</value>
    <value>Interlace</value>
    <value>Media black point</value
    <value>Media white point</value>
    <value>Overall kurtosis</value>
    <value>Overall max</value>
    <value>Overall mean</value>
    <value>Overall max</value>
    <value>Overall min</value>
    <value>Overall standard deviation</value>
    <value>Luminance</value>
    <value>Software</value>
    <value>Resolution unit</value>
    <value>Thumbnail</value>
    <value>Measurement backing</value>
    <value>Measurement geometry</value>
    <value>Measurement flare</value>
    <value>Measurement illuminant</value>
    <value>Measurement observer</value>
    <value>Media black point</value>
    <value>Media white point</value>
    <value>Page geometry</value>
    <value>Transparent color</value>
    <value>Units</value>
    <value>Pixel units</value>
    <value>Pixels per unit x</value>
    <value>Pixels per unit y</value>
    <value>Profile file signature</value>
  </util:set>

</beans>
