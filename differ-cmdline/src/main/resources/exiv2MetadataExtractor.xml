<bean id="exiv2MetadataTransformer" class="cz.nkp.differ.compare.metadata.external.RegexpTransformer">
  <property name="mapOfRegexp">
    <map>
      <entry key="Image width (Pixels)"  value="Image size      : ([0-9]+) x [0-9]+"/>
      <entry key="Image height (Pixels)" value="Image size      : [0-9]+ x ([0-9]+)"/>
    </map>
  </property>
  <property name="listOfRegexp">
    <list>
      <value>([^:]+):([^:]+)</value>
    </list>
  </property>
</bean>

